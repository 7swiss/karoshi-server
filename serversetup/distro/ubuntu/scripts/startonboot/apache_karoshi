#!/bin/bash
#Start apache2 on boot
[ ! -f /etc/rc5.d/S03apache2_karoshi ] && ln -s /etc/init.d/apache2_karoshi /etc/rc5.d/S03apache2_karoshi 1>/dev/null
[ ! -f /etc/rc4.d/S03apache2_karoshi ] && ln -s /etc/init.d/apache2_karoshi /etc/rc4.d/S03apache2_karoshi 1>/dev/null
[ ! -f /etc/rc3.d/S03apache2_karoshi ] && ln -s /etc/init.d/apache2_karoshi /etc/rc3.d/S03apache2_karoshi 1>/dev/null
[ ! -f /etc/rc2.d/S03apache2_karoshi ] && ln -s /etc/init.d/apache2_karoshi /etc/rc2.d/S03apache2_karoshi 1>/dev/null


