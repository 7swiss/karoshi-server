#!/bin/bash
#Start apache2 on boot
[ -f /etc/rc5.d/S03apache2_karoshi ] && rm -f /etc/rc5.d/S03apache2_karoshi
[ -f /etc/rc4.d/S03apache2_karoshi ] && rm -f /etc/rc4.d/S03apache2_karoshi
[ -f /etc/rc3.d/S03apache2_karoshi ] && rm -f /etc/rc3.d/S03apache2_karoshi
[ -f /etc/rc2.d/S03apache2_karoshi ] && rm -f /etc/rc2.d/S03apache2_karoshi


