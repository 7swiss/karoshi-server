#!/bin/bash

#Make sure log files exist
touch /var/log/wtmp
chown root.utmp /var/log/wtmp

touch /var/log/btmp
touch /var/log/lastlog
touch /var/log/messages

#Cron - only allow root
echo root > /etc/cron.allow

#Create ftpusers file
echo -e "root
apache_karoshi
apt-cacher-ng
avahi-autoipd
avahi\nbackup
bin
clamav
colord
daemon
dansguardian
dhcp
dnsmasq
dovecot
dovenull
freerad
gnats
hplip
kernoops
libuuid
lightdm
list
lp
mail
man
mon
mysql
news
nslcd
ntp
postfix
proxy
pulse
rtkit
sshd
statd
sync
sys
syslog
tftp
usbmux
uucp
whoopsie
www-data
karoshi
" > /etc/ftpusers

#Deny ICMP redirect requests
sysctl -w net.ipv4.conf.all.accept_redirects=0
sysctl -w net.ipv4.conf.default.accept_redirects=0

[ `grep -c ^net.ipv4.conf.all.accept_redirects=0 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.all.accept_redirects=0 >> /etc/sysctl.conf
[ `grep -c ^net.ipv4.conf.default.accept_redirects=0 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.default.accept_redirects=0 >> /etc/sysctl.conf

#Deny invalid ip addresses
sysctl -w net.ipv4.conf.all.rp_filter=2
sysctl -w net.ipv4.conf.default.rp_filter=2 

[ `grep -c ^net.ipv4.conf.all.rp_filter=2 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.all.rp_filter=2 >> /etc/sysctl.conf
[ `grep -c ^net.ipv4.conf.default.rp_filter=2 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.default.rp_filter=2 >> /etc/sysctl.conf


#Stop routing
[ `grep -c ^net.ipv4.conf.all.send_redirects=0 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.all.send_redirects=0 >> /etc/sysctl.conf
[ `grep -c ^net.ipv4.conf.all.accept_source_route=0 /etc/sysctl.conf` = 0 ] && echo net.ipv4.conf.all.accept_source_route=0 >> /etc/sysctl.conf
[ `grep -c ^net.ipv6.conf.all.accept_source_route=0 /etc/sysctl.conf` = 0 ] && echo net.ipv6.conf.all.accept_source_route=0 >> /etc/sysctl.conf


#Change permissions in /dev
chmod 0700 /dev/rfkill
chmod 0700 /dev/fuse

#Remove openldap user account
userdel openldap

#Deny access to root in ssh
sed -i 's/PermitRootLogin yes/PermitRootLogin no/g' /etc/ssh/sshd_config

#Set permissions on xinetd.conf
chmod 0600 /etc/xinetd.conf
chmod 0600 /etc/xinetd.d/chargen
chmod 0600 /etc/xinetd.d/daytime
chmod 0600 /etc/xinetd.d/discard
chmod 0600 /etc/xinetd.d/echo
chmod 0600 /etc/xinetd.d/time

#Changing root ownership of at to root
chown root.root /usr/bin/at

#Removing setuid bit
chmod u-s /bin/su
chmod u-s /usr/bin/passwd

#Removing setgid bit
chmod g-s /usr/bin/wall


#Setting umask to 022 in /etc/init.d/rc
sed -i 's/umask 027/umask 022/g' /etc/init.d/rc

#Remove dangling symlinks
rm -f /opt/karoshi/karoshi_user/.mozilla/firefox/usihywjy.default/lock
rm -f /opt/karoshi/karoshi_user/.pulse/a18f04ac224b8a50f55e26ae00000004-runtime
rm -f /etc/alternatives/gstreamer-codec-install
rm -f /etc/blkid.tab
rm -f /etc/spamassassin/mailscanner.cf
rm -f /etc/rc6.d/K20nscd
rm -f /etc/rc0.d/K20nscd
rm -f /etc/rc0.d/K99uptimed

for DANGLY in `echo -e "
/etc/alternatives/gstreamer-codec-install
/etc/blkid.tab
/etc/spamassassin/mailscanner.cf
/etc/rc6.d/K20nscd
/etc/rc0.d/K20nscd
/etc/rc0.d/K99uptimed
/etc/rc1.d/K20nscd
/etc/rc1.d/K99uptimed
/usr/lib/tiger/systems/Linux/issue.net
/usr/lib/firefox/hyphenation
/usr/lib/ispell/default.hash
/usr/lib/ispell/default.aff
/usr/lib/x86_64-linux-gnu/libnss_db.so
/usr/bin/gstreamer-codec-install
/usr/share/squid-langpack/sr
/usr/share/squid-langpack/sl-si
/usr/share/squid-langpack/sr-sp
/usr/share/squid-langpack/sr-latn-cs
/usr/share/squid-langpack/Serbian
/usr/share/squid-langpack/vi-vn
/usr/share/gnome/help/zenity/oc/index.page
/usr/share/gnome/help/zenity/oc/list.page
/usr/share/gnome/help/zenity/oc/scale.page
